<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="400sp"
    android:layout_height="match_parent"
    tools:context=".Dialogs.show_profile_logout">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center"
    android:padding="16sp">


    <ImageView
        android:id="@+id/imgdialog"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginTop="16dp"
        android:src="@drawable/person"
        android:background="@drawable/roundimgbackground"
        android:padding="10sp"
        />

    <TextView
        android:id="@+id/logout_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello world"
        android:textSize="25sp"
        android:gravity="center"
        android:layout_marginTop="10sp"
        android:fontFamily="sans-serif-light"
        />
    <TextView
        android:id="@+id/logout_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="hello world"
        android:textSize="20sp"
        android:layout_marginTop="10sp"
        />
    <TextView
        android:id="@+id/logout_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="hello world"
        android:textSize="20sp"
        android:layout_marginTop="10sp"
        />

    <ImageButton
        android:layout_width="50sp"
        android:layout_height="50sp"
        android:src="@drawable/logout_img"
        android:background="@drawable/strokeoutline"
        android:layout_marginTop="16sp"
        android:id="@+id/logoutbtn"

        />

</LinearLayout>
</FrameLayout>